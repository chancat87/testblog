{
  "name": "blog",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "pnpm run build:client && pnpm run build:server && pnpm run build:static",
    "build:client": "vite build --outDir dist/client --manifest",
    "build:server": "vite build --outDir dist/server --ssr app/entry.server.tsx",
    "build:static": "vite build --outDir dist/static --ssr -c adapters/static/vite.config.ts && cp -r public/* dist/static && cp -r dist/client/build dist/static",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check ."
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@biliblitz/blitz": "^0.0.3",
    "@biliblitz/node-server": "^0.0.3",
    "@hono/node-server": "^1.5.0",
    "@mapbox/rehype-prism": "^0.9.0",
    "@mdx-js/preact": "^3.0.0",
    "@preact/signals": "^1.2.2",
    "preact": "^10.19.3",
    "preact-render-to-string": "^6.3.1",
    "rehype-katex": "^6.0.2",
    "remark-math": "^6.0.0",
    "remark-ruby": "^0.4.0"
  },
  "devDependencies": {
    "@biliblitz/vite": "^0.0.3",
    "@preact/preset-vite": "^2.8.1",
    "@types/node": "^20.9.0",
    "autoprefixer": "^10.4.17",
    "katex": "^0.16.9",
    "postcss": "^8.4.34",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "remark-gfm": "^4.0.0",
    "tailwindcss": "^3.4.1",
    "vite": "^5.0.12",
    "vite-tsconfig-paths": "^4.3.1"
  },
  "resolutions": {
    "katex": "0.15.6"
  }
}
